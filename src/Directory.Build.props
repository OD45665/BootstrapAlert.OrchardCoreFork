<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <PropertyGroup>
        <PackageId>BootstrapAlert.OrchardCore</PackageId>
        <Title>Bootstrap Alert Widget for Orchard Core</Title>
        <Description>Adds a recipe that creates a Bootstrap Alert widget for Orchard Core CMS Website.</Description>
        <PackageTags>Alert;Bootstrap;OrchardCore</PackageTags>
        <VersionPrefix>1.0.0</VersionPrefix>
        <VersionSuffix>rc2</VersionSuffix>
    </PropertyGroup>

    <!-- Called after so that the <VersionSuffix> is built after the local ones -->
    <Import Project="Common.props"/>

</Project>